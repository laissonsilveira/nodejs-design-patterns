{
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-empty": "error",
        "no-cond-assign": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}